{
  "editor.tabSize": 2,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "prettier.singleQuote": true,
  "prettier.endOfLine": "lf",
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "none",
  "prettier.arrowParens": "always",
  "prettier.packageManager": "yarn",
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks"
}
